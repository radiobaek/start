<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.ozsoft.secs4j.plugin.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="org.ozsoft.secs4j.plugin.Perspective"
            id="org.ozsoft.secs4j.plugin.perspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.ozsoft.secs4j.plugin.application"
            name="org.ozsoft.secs4j.plugin.product">
         <property
               name="appName"
               value="org.ozsoft.secs4j.plugin.product">
         </property>
      </product>
   </extension>

</plugin>
